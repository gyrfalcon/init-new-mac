[user]
	name = Justin Shepard
[alias]
	staged = diff --cached
	fast-amend = commit --amend --no-edit
	unstage = reset HEAD --
	tip = log -1
	tidy = !git branch --merged | sed /^\\*/d | xargs git branch -d
	branchg = !git branch -avv | grep
[core]
	excludesfile = ~/.gitignore
[fetch]
	prune = true
[protocol]
	version = 2
